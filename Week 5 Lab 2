

		import java.util.Scanner;

		public class Week5Lab2 {

		 public static void main(String[] args) {

		Scanner inp = new Scanner( System.in );

		int userNum;

		int computerNum;

		

		computerNum = 0 + (int) ( Math.random() * 1 );

		System.out.println ("Enter a number between 0 and 10");

		userNum = inp.nextInt();

		System.out.printf("Random number is %d\n", computerNum );
		
		
		if (userNum == computerNum) { System.out.println ("Match");
		
				
		if (userNum > computerNum)  System.out.println ("Too big");
		
		}

		else  { System.out.println ("Too Small");

		
		
		}}}

	
		
		

		
		
		

		
	


